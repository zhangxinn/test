 import java.util.Scanner;

public class StringRev {

   static Scanner cin=new Scanner(System.in);

   public static void main(String[] args) {
      String s1=cin.nextLine();
      String ans="";
      int L=s1.length();
      for(int i=L-1; i>=0; i--) {
         ans=ans+s1.charAt(i);
      }
      System.out.println(ans); 
   }

}

